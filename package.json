{
  "name": "react-clusterize",
  "version": "0.2.2",
  "description": "A Clusterize.js component for React",
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "prepublish": "npm run lint && npm test && npm run clean && npm run bowersync && npm run build && npm run build-examples",
    "bowersync": "./scripts/bowersync",
    "build": "babel --out-dir ./lib ./src",
    "build-examples": "cd examples; webpack",
    "clean": "rm -f lib/*",
    "demo": "http-server -p 8000 docs/",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "tap test/*.js --node-arg=--require --node-arg=babel-register --node-arg=--require --node-arg=babel-polyfill",
    "coveralls": "tap test/*.js --coverage --coverage-report=text-lcov --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill | coveralls",
    "dev": "cd examples; webpack-dev-server --hot --inline --host 0.0.0.0 --port 8000 --content-base ../docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheton/react-clusterize.git"
  },
  "author": "Cheton Wu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheton/react-clusterize/issues"
  },
  "homepage": "https://github.com/cheton/react-clusterize",
  "keywords": [
    "cluster",
    "clusterize",
    "list",
    "react",
    "react-clusterize",
    "scroll",
    "table"
  ],
  "peerDependencies": {
    "react": "^0.14.0 || >=15.0.0",
    "react-dom": "^0.14.0 || >=15.0.0"
  },
  "dependencies": {
    "clusterize.js": "^0.17.6",
    "prop-types": "^15.5.8"
  },
  "devDependencies": {
    "@trendmicro/react-buttons": "^1.0.3",
    "babel-cli": "~6.24.1",
    "babel-core": "~6.24.1",
    "babel-eslint": "~7.2.2",
    "babel-loader": "~6.4.1",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "coveralls": "~2.13.0",
    "css-loader": "~0.28.0",
    "eslint": "~3.19.0",
    "eslint-config-trendmicro": "~0.5.1",
    "eslint-loader": "~1.7.1",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~2.2.3",
    "eslint-plugin-react": "~6.10.3",
    "file-loader": "~0.11.1",
    "html-webpack-plugin": "~2.28.0",
    "http-server": "~0.9.0",
    "nib": "~1.1.2",
    "react": "^0.14.0 || >=15.0.0",
    "react-dom": "^0.14.0 || >=15.0.0",
    "style-loader": "~0.16.1",
    "stylint": "~1.5.9",
    "stylint-loader": "~1.0.0",
    "stylus-loader": "~3.0.1",
    "tap": "~10.3.2",
    "trendmicro-ui": "~0.4.0",
    "url-loader": "~0.5.8",
    "webpack": "~2.4.1",
    "webpack-dev-server": "~2.4.2",
    "which": "~1.2.14"
  }
}
